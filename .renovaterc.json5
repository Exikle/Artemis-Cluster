{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  description: 'Renovate configuration for Artemis-Cluster',
  extends: [
    'config:recommended',
    'docker:enableMajor',
    'helpers:pinGitHubActionDigests',
    'github>Exikle/Artemis-Cluster//.renovate/autoMerge.json5',
    'github>Exikle/Artemis-Cluster//.renovate/commitMessages.json5',
    'github>Exikle/Artemis-Cluster//.renovate/groups.json5',
    'github>Exikle/Artemis-Cluster//.renovate/labels.json5',
    'github>Exikle/Artemis-Cluster//.renovate/customManagers.json5',
    ':disableRateLimiting',
    ':dependencyDashboard',
    ':semanticCommits',
    ':automergeBranch',
    ':timezone(America/Toronto)',
  ],
  prConcurrentLimit: 10,
  branchConcurrentLimit: 5,
  prCreation: 'not-pending',
  dependencyDashboard: true,
  dependencyDashboardTitle: 'Renovate Dashboard ðŸ¤–',
  suppressNotifications: [
    'prEditedNotification',
    'prIgnoreNotification',
  ],
  schedule: [
    'every weekday',
  ],
  automergeSchedule: [
    'every weekday',
  ],
  rebaseWhen: 'conflicted',
  ignorePaths: [
    '**/*.sops.*',
    '**/*.secret.*',
  ],
  flux: {
    managerFilePatterns: [
      '/(^|/)kubernetes/.+\\.ya?ml(?:\\.j2)?$/',
    ],
  },
  'helm-values': {
    managerFilePatterns: [
      '/(^|/)kubernetes/.+\\.ya?ml(?:\\.j2)?$/',
    ],
  },
  helmfile: {
    managerFilePatterns: [
      '/(^|/)helmfile\\.ya?ml(?:\\.j2)?$/',
    ],
  },
  kubernetes: {
    managerFilePatterns: [
      '/(^|/)kubernetes/.+\\.ya?ml(?:\\.j2)?$/',
    ],
  },
  kustomize: {
    managerFilePatterns: [
      '/(^|/)kustomization\\.ya?ml(?:\\.j2)?$/',
    ],
  },
  pip_requirements: {
    managerFilePatterns: [
      '/(^|/)[\\w-]*requirements(-\\w+)?\\.(txt|pip)(?:\\.j2)?$/',
    ],
  },
}
